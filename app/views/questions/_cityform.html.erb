<%= form_for([@city, @city.questions.build]) do |f| %>
        <div class="field">
                <%= f.label :question %><br />
                <%= f.text_field :question %>
        </div>
        <div class="field">
                <%= f.label :detail %><br />
                <%= f.text_area :detail %>
        </div>
        <div class="actions">
                <%= f.submit %>
        </div>
<% end %>
